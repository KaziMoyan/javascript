//Arry loop
let num=[3,4,56,7,90]
for(let i=0;i<num.length;i++){
  console.log(num[i])
}

num.forEach((element)=>{
  console.log(element*element)
})
//arry From
let name="Moyeen"
let arr = Array.from(name)
console.log(arr)

// for..of 
for(let i of num){
  console.log(i)
}

//for ...in
for(let i in num){
  console.log(num[i])
}
